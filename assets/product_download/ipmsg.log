    function notifyMe(res,user_name,user_id) {
        // new Audio('<?php echo base_url()."assets/noti_sound/noti.mp3";?>').play();

        if (!("Notification" in window)) {
            alert("This browser does not support desktop notification");
        }else if(Notification.permission === "granted") {
            var options = {
                body: res,
                icon: "assets/images/cirklesmall.png",
                dir: "ltr"
            };
            var notification = new Notification(user_name, options);

            notification.onclick = function () {
                if(middle=='chat_view'){
                    change_chat(user_id);
                    window.focus();
                }else{
                    window.focus();
                }
            };
            setTimeout(function() { notification.close() }, 4000);

        }else if(Notification.permission !== 'denied') {
            Notification.requestPermission(function (permission) {
                if (!('permission' in Notification)) {
                    Notification.permission = permission;
                }

                if(permission === "granted"){
                    var options = {
                        body: res,
                        icon: "assets/images/cirklesmall.png",
                        dir: "ltr"
                    };
                    var notification = new Notification(user_name, options);
                }
            });
        }
    }

