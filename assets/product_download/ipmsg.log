public function custompagination($page =1){
        {
            $qur = "select * from datatable";
            $res = $this->db->query($qur)->result_array();
            $totalblogs = sizeof($res);
            $per_page = 10;
            $totallink = ceil($totalblogs / $per_page);

            $paglinks = '<ul class="pagination">';
            if ($page == 1) {
                $paglinks .= '<li class="page-item disabled"><a class="page-link" href="javascript:void(0)" tabindex="-1">Previous</a></li>';
            } else {
                $paglinks .= '<li class="page-item"><a class="page-link" href="' . base_url() . 'admin/pagination/custompagination/' . ($page - 1) . '" tabindex="-1">Previous</a></li>';
            }

            for ($i = 1; $i <= $totallink; $i++) {
                if ($page == $i) {
                    $class = 'active-item';
                } else {
                    $class = '';
                }
                $paglinks .= '<li class="page-item"><a class="page-link ' . $class . '" href="' . base_url() . 'admin/pagination/custompagination/' . $i . '">' . $i . '</a></li>';
            }

            if ($totallink == $page) {
                $paglinks .= '<li class="page-item disabled"><a class="page-link"  href="javascript:void(0)">Next</a></li>';
            } else {
                $paglinks .= '<li class="page-item"><a class="page-link"  href="' . base_url() . 'admin/pagination/custompagination/' . ($page + 1) . '">Next</a></li>';
            }
            $paglinks .= '</ul>';
            $data['paglinks'] = $paglinks;

            if ($page == 1) {
                $start = 0;
                $end = $per_page;
            } else {
                $start = ($page - 1) * $per_page;
                $end = $per_page;
            }

            $res = $this->db->query("select * from datatable limit " . $start . "," . $end)->result_array();
            $data['userdetails'] = $res;
            $data['middle'] = 'admin/pagination/cutompagination';
            $this->load->view('admin/template', $data);
        }
    }

