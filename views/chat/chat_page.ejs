<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>Node Chat Applocation Dashboard</title>
    <meta content="Admin Dashboard" name="description" />
    <meta content="Mannatthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <link href="assets/plugins/morris/morris.css" rel="stylesheet">

    <link href="assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="assets/css/style.css" rel="stylesheet" type="text/css">

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i,900,900i"
        rel="stylesheet">

    <link href="assets/css/chat_page/style.css" rel="stylesheet" type="text/css">

</head>
<!-- user data in session here -->
<% user_session.forEach(function(data){ %>
<!-- commented tag  -->
<%#  console.log(data.user_file) %>
<%#  data.user_file %>

<body class="fixed-left">

    <!-- Loader -->
    <div id="preloader">
        <div id="status">
            <div class="spinner"></div>
        </div>
    </div>

    <!-- Begin page -->
    <div id="wrapper">

        <!-- ========== Left Sidebar Start ========== -->
        <% include left_header.ejs %>
        <!-- Left Sidebar End -->

        <!-- Start right Content here -->

        <div class="content-page">
            <!-- Start content -->
            <div class="content">

                <!-- Top Bar Start -->
                <% include top_header.ejs %>

                <!-- Top Bar End -->

                <div class="page-content-wrapper ">

                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-sm-12">
                                <div class="page-title-box">
                                    <h4 class="page-title">Chat Dashboard</h4>
                                </div>
                            </div>
                        </div>
                        <!-- end page title end breadcrumb -->
                        <!--start middile part -->
                        <div class="container">
                            <div class="UserList" style="left: 249px;top: 136px;">
                                <ul>
                                    <li class="menu-title">Online</li>
                                    <div id="online_user_list"></div>
                                    <li class="menu-title">Offline</li>
                                    <div id="offline_user_list"></div>
                                </ul>
                            </div>
                            <div class="ChatMessages">
                                <div class="BoxTitle">Messages</div>
                                <div class="active_user_id"></div>
                                <div class="Chating" style="height: 623px;">
                                    <div class="data_limit"><input type='hidden' id='data_limit' name='data_limit'
                                            value='2'></div>
                                    <img src="assets/images/logout.png" alt="loader not found" id='loader'
                                        style="display:none">
                                    <ul class="UIList">

                                    </ul>

                                </div>
                                <div class="ChatCard">
                                    <!-- <form> -->
                                    <div class="TypeBox"><textarea id="chat_txt"
                                            placeholder="Type your message here.."></textarea></div>
                                    <div class="Btn"><button type="button" class="SendBtn" onclick="btn_click();"><i
                                                class="ion ion-paper-airplane"></i></button></div>
                                    <!-- </form> -->
                                </div>
                            </div>
                        </div>
                        <!--end middile part -->

                    </div><!-- container -->
                </div> <!-- Page content Wrapper -->
            </div> <!-- content -->
            <% include script_footer.ejs %>

</body>
<!-- user data session end here -->
<% }); %>

</html>