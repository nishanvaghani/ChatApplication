<body class="fixed-left">

<!-- Begin page -->
<div class="accountbg"></div>
<div class="wrapper-page">

    <div class="card">
        <div class="card-body">

            <h3 class="text-center mt-0 m-b-15">
                <a href="javascript:void(0);" class="logo logo-admin">LogIn</a>
            </h3>

            <div class="p-3">
                <span style="color:red;" class="error"><% if(error_msg) { %> <%= error_msg; %> <% } %></span>
                <form class="form-horizontal m-t-20" action="http://localhost:2005/verify_login" method="post" >
                    <label class="control-label col-sm-12" for="name" style="padding: 0px">Username:</label>
                    <div class="form-group row">
                        <div class="col-12">
                            <input class="form-control" id="name" name='username' type="text" required="" placeholder="Username">
                        </div>
                    </div>
                    <label class="control-label col-sm-12" for="name" style="padding: 0px">Password:</label>
                    <span class=" mdi mdi-eye show_pass" style="cursor: pointer"> Show Password</span>
                    <span class=" mdi mdi-eye-off hide_pass" style="cursor: pointer; display: none"> Hide Password</span>
                    <div class="form-group row">
                        <div class="col-12">
                            <input class="form-control" type="password" id="pass" name="password" required="" placeholder="Password">
                        </div>
                    </div>

                    <div class="form-group text-center row m-t-20">
                        <div class="col-12">
                            <button class="btn btn-success btn-block waves-effect waves-light" id="login_id" type="submit">Log In</button>
                        </div>
                    </div>
                    <div class="form-group text-center row m-t-20">
                        <div class="col-12">
                            <a href="http://localhost:2005/register" class="btn btn-danger btn-block waves-effect waves-light">Register</a>
                        </div>
                    </div>

                    <!--                    <div class="form-group m-t-10 mb-0 row">-->
                    <!--                        <div class="col-sm-7 m-t-20">-->
                    <!--                            <a href="pages-recoverpw.html" class="text-muted"><i class="mdi mdi-lock"></i> <small>Forgot your password ?</small></a>-->
